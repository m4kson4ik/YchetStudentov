        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YchetStudentov.Form"
        mc:Ignorable="d"
        Title="Редактирование дисциплины" Height="280" Width="286" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="btEditing" Content="Изменить" HorizontalAlignment="Center" Margin="0,212,0,0" VerticalAlignment="Top" Click="Button_Click" Background="#FF3AB78A" BorderBrush="#FF5DC08A"/>
        <TextBox x:Name="tbNameDisciplini" HorizontalAlignment="Left" Margin="36,35,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="198" Height="38"/>
        <ComboBox x:Name="cmbTeacher" HorizontalAlignment="Left" Margin="36,157,0,0" VerticalAlignment="Top" Width="198" DisplayMemberPath="fio"/>
        <ComboBox x:Name="cmbFormaAttest" HorizontalAlignment="Left" Margin="36,106,0,0" VerticalAlignment="Top" Width="198"/>

    </Grid>
</Window>
